{
  "manifest_version": 2,

  "name": "Go your JIRAs!",
  "description": "☛ Simple to use, GoJira can help you enter and track you jira tickets easily! ☚",
  "version": "2.0.0",

  "browser_action": {
    "default_title": "Go your JIRAs!",
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },

  "options_page": "options.html",

  "permissions": [
     "activeTab",
     "storage"
   ]
}